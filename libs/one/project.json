{
  "root": "one",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "echo one:build",
            "forwardAllArgs": false
          },
          {
            "command": "mkdir -p ./dist",
            "forwardAllArgs": false
          },
          {
            "command": "touch ./dist/one",
            "forwardAllArgs": false
          }
        ],
        "parallel": false
      }
    }
  }
}
