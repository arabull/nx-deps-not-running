{
  "root": "two",
  "implicitDependencies": ["one"],
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "echo two:build",
            "forwardAllArgs": false
          },
          {
            "command": "mkdir -p ./dist",
            "forwardAllArgs": false
          },
          {
            "command": "touch ./dist/two",
            "forwardAllArgs": false
          }
        ],
        "parallel": false
      }
    },
    "generate-sources": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          { "command": "echo two:generate-sources", "forwardAllArgs": false },
          { "command": "test -e ./dist/one", "forwardAllArgs": false }
        ],
        "parallel": false
      }
    }
  }
}
